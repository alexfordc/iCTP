        <input type="usercontrol" cid="ribbonbutton2" name="btnAddCategory" location="9,325" size="100,29" text="添加类别" />
        <!--添加类别按钮-->
        <input type="usercontrol" cid="ribbonbutton2" name="btnDeleteCategory" location="119,325" size="100,29" text="删除类别" />
        <!--合约类别表格-->
        <table name="gridCategory" autoellipsis="true" backcolor="empty" celleditmode="DoubleClick" gridlinecolor="empty" size="210,270" location="9,50" headervisible="false" showvscrollbar="true">
